{
  "extends": ["html-validate:recommended"],
  "rules": {
    "attr-quotes": "off",
    "doctype-style": "off",
    "element-required-attributes": "off",
    "no-inline-style": "off",
    "no-raw-characters": "off",
    "no-trailing-whitespace": "off",
    "prefer-native-element": [ "error", {
      "exclude": [ "button", "section" ]
    }],
    "svg-focusable": "off",
    "valid-id": "off",
    "void-style": "off",
    "wcag/h32": "off",
    "wcag/h63": "off"
  }
}