{
  "name": "univ-medicale",
  "version": "0.2.0",
  "description": "Site communautaire des recommandations en premier recours en médecine générale",
  "scripts": {
    "build": "hugo --gc --minify",
    "css:purge": "purgecss -c build/postcss.config.js",
    "html:lint": "htmllint --rc build/.htmllintrc \"public/**/*.html\"",
    "server": "hugo server -D -w -d dev --verbose"
  },
  "keywords": [
    "hugo",
    "open",
    "source",
    "site",
    "recommandations",
    "medecine",
    "conduite",
    "à",
    "tenir",
    "médecine",
    "guidelines"
  ],
  "author": "Jean-Baptiste FRON",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/djibe/recommandations-medicales.git"
  },
  "bugs": {
    "url": "https://github.com/djibe/recommandations-medicales/issues"
  },
  "homepage": "https://recommandations-medicales.netlify.app",
  "devDependencies": {
    "htmllint-cli": "0.0.7",
    "vnu-jar": "^20.6.30"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "autoprefixer": "^10.0.2",
    "postcss-cli": "^8.3.0"
  },
  "browserslist": [
    "defaults"
  ]
}
